<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.help.toc">
      <toc file="toc.xml"/>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="com.jrockit.mc.console.ui.notification_contexts.xml"
            plugin="com.jrockit.mc.console.ui.notification"/>
      <contexts
            file="com.jrockit.mc.console.ui.notification.tab_contexts.xml"
            plugin="com.jrockit.mc.console.ui.notification.tab"/>
   </extension>
   <extension
         point="com.jrockit.mc.core.formpage">
      <formPage
            class="com.jrockit.mc.console.ui.notification.TriggerTab"
            hostEditorId="com.jrockit.mc.console.ui.editor"
            icon="icons/alert_obj.gif"
            id="com.jrockit.mc.console.ui.notification.TriggerTab"
            title="%TAB_TRIGGER_NAME">
      </formPage>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:com.jrockit.mc.console.ui.editor.mbeans">
         <command
               commandId="com.jrockit.mc.ui.formpage.default.command"
               icon="icons/alert_obj.gif"
               id="triggers"
               label="%TAB_TRIGGER_NAME">
            <parameter
                  name="formpage"
                  value="com.jrockit.mc.console.ui.notification.TriggerTab">
            </parameter>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            category="com.jrockit.mc.ui.wizards.export"
            class="com.jrockit.mc.console.ui.notification.wizard.RuleExportWizard"
            icon="icons/alert_obj.gif"
            id="com.jrockit.mc.console.ui.notification.RuleExportWizard"
            name="%TRIGGER_RULES">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="com.jrockit.mc.ui.wizards.import"
            class="com.jrockit.mc.console.ui.notification.wizard.RuleImportWizard"
            icon="icons/alert_obj.gif"
            id="com.jrockit.mc.console.ui.notification.RuleImportWizard"
            name="%TRIGGER_RULES">
      </wizard>
   </extension>
   <extension
         point="com.jrockit.mc.rjmx.core.triggerActions">
      <triggerAction
            class="com.jrockit.mc.console.ui.notification.action.TriggerActionJFR"
            description="%TRIGGER_ACTION_JFR_DESCRIPTION"
            icon="icons/flight_recorder.gif"
            id="com.jrockit.mc.console.ui.notification.actions.TriggerActionThreadStackDump"
            name="%TRIGGER_ACTION_JFR_NAME">
         <string
               description="%TRIGGER_ACTION_JFR_NAME_DESCRIPTION"
               id="name"
               name="%TRIGGER_ACTION_JFR_NAME_LABEL"
               value="JRockit default"/>
         <file
               description="%TRIGGER_ACTION_JFR_FILE_DESCRIPTION"
               id="file"
               name="%TRIGGER_ACTION_JFR_FILE"
               value="automaticallyTriggeredRecording.jfr"/>
         <timerange
               id="timerange"
               name="%TRIGGER_ACTION_TIMERANGE_LABEL"
               description="%TRIGGER_ACTION_TIMERANGE_DESCRIPTION"
               value="30s"/>
      </triggerAction>
   </extension>
</plugin>
